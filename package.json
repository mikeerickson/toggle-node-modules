{
  "name": "toggle-node-modules",
  "version": "1.1.1",
  "main": "./lib/main",
  "description": "Quickly toggle (hide | show) node modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeerickson/toggle-node-modules.git"
  },
  "license": "MIT",
  "activationCommands": {
    "atom-text-editor": [
      "toggle-node-modules:toggle"
    ]
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/mikeerickson/toggle-node-modules/issues"
  },
  "homepage": "https://github.com/mikeerickson/toggle-node-modules#readme",
  "_id": "toggle-node-modules@1.0.0",
  "_atomModuleCache": {
    "version": 1,
    "dependencies": [],
    "extensions": {
      ".json": [
        "package.json"
      ],
      ".js": [
        "lib/main.js"
      ]
    },
    "folders": []
  }
}
